#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Applications/javaProject/vuepress-starter/node_modules/.pnpm/vuepress@2.0.0-rc.15_@vuepress+bundler-vite@2.0.0-rc.15_@types+node@22.5.0_sass@1.77.8__vue@3.4.38/node_modules/vuepress/bin/node_modules:/Applications/javaProject/vuepress-starter/node_modules/.pnpm/vuepress@2.0.0-rc.15_@vuepress+bundler-vite@2.0.0-rc.15_@types+node@22.5.0_sass@1.77.8__vue@3.4.38/node_modules/vuepress/node_modules:/Applications/javaProject/vuepress-starter/node_modules/.pnpm/vuepress@2.0.0-rc.15_@vuepress+bundler-vite@2.0.0-rc.15_@types+node@22.5.0_sass@1.77.8__vue@3.4.38/node_modules:/Applications/javaProject/vuepress-starter/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Applications/javaProject/vuepress-starter/node_modules/.pnpm/vuepress@2.0.0-rc.15_@vuepress+bundler-vite@2.0.0-rc.15_@types+node@22.5.0_sass@1.77.8__vue@3.4.38/node_modules/vuepress/bin/node_modules:/Applications/javaProject/vuepress-starter/node_modules/.pnpm/vuepress@2.0.0-rc.15_@vuepress+bundler-vite@2.0.0-rc.15_@types+node@22.5.0_sass@1.77.8__vue@3.4.38/node_modules/vuepress/node_modules:/Applications/javaProject/vuepress-starter/node_modules/.pnpm/vuepress@2.0.0-rc.15_@vuepress+bundler-vite@2.0.0-rc.15_@types+node@22.5.0_sass@1.77.8__vue@3.4.38/node_modules:/Applications/javaProject/vuepress-starter/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/vuepress-vite.js" "$@"
else
  exec node  "$basedir/../../bin/vuepress-vite.js" "$@"
fi
